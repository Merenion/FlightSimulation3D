      <SubScene fx:id="drawScene" depthBuffer="true" depthTest="ENABLE" height="600.0" layoutX="14.0" pickOnBounds="false" width="1200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="-14.0" AnchorPane.topAnchor="0.0">
