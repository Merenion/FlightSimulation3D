      <Accordion fx:id="menu" layoutX="995.0" prefHeight="600.0" prefWidth="205.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <panes>
          <TitledPane animated="false" text="Simulation control">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Slider fx:id="sliderDeltaTimeFactor" layoutX="39.0" layoutY="74.0" majorTickUnit="1.0" max="1000.0" min="1.0" minorTickCount="100" value="50.0" />
